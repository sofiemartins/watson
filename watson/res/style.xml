<?xml version="1.0" encoding="UTF-8"?>
<synth>
	<style id="toolbarButtonStyle">
		<opaque value="TRUE" />
		<insets top="10" left="10" right="10" bottom="10" />
		<state>
			<color value="WHITE" type="BACKGROUND" />
			<color value="#444444" type="FOREGROUND" />
		</state>
		<state value="MOUSE_OVER">
			<color value="#EEEEFF" type="BACKGROUND" />
			<color value="#222222" type="FOREGROUND" />
		</state>
	</style>
	
	<style id="panelStyle">
		<insets top="5" left="5" right="5" bottom="5" />
		<state>
			<color value="WHITE" type="BACKGROUND" />
			<color value="#222222" type="FOREGROUND" />
		</state>
	</style>
	
	<style id="backgroundPanelStyle">
		<opaque value="TRUE" />
		<insets top="5" left="5" right="5" bottom="5" />
		<state>
			<imagePainter method="frameBackground" path="gradient.png" sourceInsets="10 10 10 10 " />
		</state> 
	</style>
	
	<style id="listStyle">
		<opaque value="TRUE" />
		<state>
			<color value="WHITE" type="BACKGROUND" />
			<color value="BLACK" type="FOREGROUND" />
		</state>
	</style>
	
	<style id="backingStyle">
		<opaque value="TRUE" />
		<font name="Dialog" size="14" />
		<state>
			<color value="WHITE" type="BACKGROUND" />
		</state>
	</style>
	<bind style="backingStyle" type="region" key=".*" />
	<bind style="panelStyle" type="region" key="Panel" />
	<bind style="listStyle" type="region" key="List" />
	<bind style="toolbarButtonStyle" type="name" key="toolbarButton" />
	<bind style="backgroundPanelStyle" type="name" key="Frame" />
</synth>
